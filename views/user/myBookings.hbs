      <th scope="col">Boarding Date</th>
      <th scope="col">Return Date</th>
      <th scope="col">Total Price</th>
      <th scope="col"></th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>

    
    {{#each active}}
    <tr>
      <th scope="col">{{this.packages.name}}</th>
      <td>{{this.boarding}}</td>
      <td>{{this.return}}</td>
      <td>{{this.booking.totalPrice}}</td>
      <td><a href="/booking/view/{{this._id}}"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-eye" viewBox="0 0 16 16">
